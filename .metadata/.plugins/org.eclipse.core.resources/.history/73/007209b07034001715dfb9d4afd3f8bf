package com.hellow;

public class fei {
	
	public int add(int n,int curren,int prevValue){
		
		if (n <= 0){
			return 1;
		}
		int a = prevValue;
		int b = curren;
		
		return add(n-1,b,b+a);
		
	}
	
	public static void main(String[] args) {
		
		fei a = new fei();
		
		int b = a.add(3, 1, 1);
		System.out.println(b);
	}
	
}
